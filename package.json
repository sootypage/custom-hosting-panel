{
  "name": "custom-hosting-panel",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "apps/node"
  ],
  "devDependencies": {
    "turbo": "^2.3.4",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "install:all": "npm install --workspaces",
    "start": "docker compose up -d --build",
    "dev": "docker compose up --build",
    "stop": "docker compose down",
    "logs": "docker compose logs -f"
  }
}
